    set /a "hue=%%i*360/100"
    set /a "colorCode=16 + (%%i*6) %% 200"
    call :color_line !colorCode! "!line[%%i]!"
    timeout /t 2 >nul
)

echo.
echo [38;5;226mâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• END OF CREDITS â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
echo.
pause
goto menu

:color_line
:: %1 = color code 16-231
:: %2 = message
echo [38;5;%1m%2
exit /b


:not_ready
cls
color 4
echo.
