echo                 [09]Metadata Extractor              [19]WHOIS Domain Lookup             [29]Password Strength Checker
echo                 [10]Shodan Search                   [20]HTML Source                     [30]CrÃ©dits
echo.
echo.
set /p input=.%BS% â–ˆâ–ˆâ–ˆâ•â•â•â•^>
if /i %input% EQU 1 goto start_usernames
